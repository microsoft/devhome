    <x:Double x:Key="WindowTitleBarIconHeight">16</x:Double>
    <x:Double x:Key="WindowTitleBarIconWidth">16</x:Double>
    <x:Double x:Key="WindowTitleBarComponentSpacing">16</x:Double>
    <Thickness x:Key="WindowTitleBarOffset">16,0</Thickness>
